
@{
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Admin Paneli</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i">
    <link rel="stylesheet" href="~/Content/assets/css/ready.css">
    <link rel="stylesheet" href="~/Content/assets/css/demo.css">
</head>
<body>
    <div class="wrapper">
        <div class="main-header">
            <div class="logo-header">
                Admin Paneli
            </div>
            <nav class="navbar navbar-header navbar-expand-lg">
                <div class="container-fluid">

                    <form class="navbar-left navbar-form nav-search mr-md-3" action="">
                        <div class="input-group">
                            <input type="text" placeholder="Arama Yap..." class="form-control">
                            <div class="input-group-append">
                                <span class="input-group-text">
                                    <i class="la la-search search-icon"></i>
                                </span>
                            </div>
                        </div>
                    </form>
                    <ul class="navbar-nav topbar-nav ml-md-auto align-items-center">
                        <li>
                            <a class="dropdown-toggle profile-pic" href="#" aria-expanded="false"> <img src="~/Content/assets/img/pp.jpg" alt="user-img" width="36" class="img-circle"><span>Hatice Çelik</span> </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
        <div class="sidebar">
            <div class="scrollbar-inner sidebar-wrapper">
                <div class="user">
                    <div class="photo">
                        <img src="~/Content/assets/img/pp.jpg">
                    </div>
                    <div class="info">
                        <a class="" data-toggle="collapse" href="#collapseExample" aria-expanded="true">
                            <span>
                                Hatice ÇELİK
                                <span class="user-level">Administrator</span>
                                <span class="caret"></span>
                            </span>
                        </a>
                        <div class="clearfix"></div>

                        <div class="collapse in" id="collapseExample" aria-expanded="true" style="">
                            <ul class="nav">
                                <li>
                                    <a href="#profile">
                                        <span class="link-collapse">Profilim</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#edit">
                                        <span class="link-collapse">Profili Düzenle</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#settings">
                                        <span class="link-collapse">Çıkış Yap</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

                <ul class="nav">
                    <li class="nav-item active">
                        <a href="/AdminHome/Index/">
                            <i class="la la-dashboard"></i>
                            <p>Ana Sayfa</p>
                            <span class="badge badge-count"></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="/Customer/Index/">
                            <i class="la la-group "></i>
                            <p>Müşteriler</p>
                            <span class="badge badge-count"></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="/Category/Index">
                            <i class="la la-list"></i>
                            <p>Kategoriler</p>
                            <span class="badge badge-count"></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="/Employee/Index/">
                            <i class="la la-user"></i>
                            <p>Personel</p>
                            <span class="badge badge-count"></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href="/Product/Index/">
                            <i class="la la-inbox"></i>
                            <p>Ürünler</p>
                            <span class="badge badge-success"></span>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a href="/Login/Index/">
                            <i class="la la-user-plus"></i>
                            <p>Giriş Yap</p>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a href="#">
                            <i class="la la-mail-reply"></i>
                            <p>Çıkış Yap</p>
                        </a>
                    </li>

                </ul>
            </div>

        </div>

        <div class="main-panel">
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
           
        </div>
        </div>




</body>
<script src="~/Content/assets/js/core/jquery.3.2.1.min.js"></script>
<script src="~/Content/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
<script src="~/Content/assets/js/core/popper.min.js"></script>
<script src="~/Content/assets/js/core/bootstrap.min.js"></script>
<script src="~/Content/assets/js/plugin/chartist/chartist.min.js"></script>
<script src="~/Content/assets/js/plugin/chartist/plugin/chartist-plugin-tooltip.min.js"></script>
<script src="~/Content/assets/js/plugin/bootstrap-toggle/bootstrap-toggle.min.js"></script>
<script src="~/Content/assets/js/plugin/jquery-mapael/jquery.mapael.min.js"></script>
<script src="~/Content/assets/js/plugin/jquery-mapael/maps/world_countries.min.js"></script>
<script src="~/Content/assets/js/plugin/chart-circle/circles.min.js"></script>
<script src="~/Content/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>
<script src="~/Content/assets/js/ready.min.js"></script>
<script src="~/Content/assets/js/demo.js"></script>
</html>
